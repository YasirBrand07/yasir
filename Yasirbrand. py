def SXD():
    os.system('clear')
    print(logo)
    print(f'[1] Version 9.9.9')
    print(f'[2] Version 9.8.8')
    print(f'[3] 32 Bit ')
    print(47*'-')
    cho = input('Select Version: ')
    if cho == '1':
       if bit == '64bit':
           if not os.path.isfile('YYB.so'):
               os.system('curl -L https://github.com/Yasir-XD/executables/blob/main/YYB.cpython-310.so?raw=true -o YYB.so') 
               import YYB
           else:
               import YYB
    if cho == '2':
       if bit == '64bit':
           if not os.path.isfile('Yasir.so'):
               os.system('curl -L https://github.com/Yasir-XD/executables/blob/main/Yasir.cpython-310.so?raw=true -o Yasir.so') 
               import Yasir
           else:
               import Yasir
    if cho == '3':
       if bit == '32bit':
           if not os.path.isfile('Yasirbrand07.so'):
               os.system('curl -L https://github.com/Yasir-XD/executables/blob/main/Yasirbrand07.cpython-310.so?raw=true -o Yasirbrand07.so') 
               import Yasirbrand07
           else:
               import Yasirbrand07
    
        
SXD()

